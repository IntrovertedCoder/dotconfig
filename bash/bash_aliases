# Custom aliases
alias ..='cd ..'
alias v='vim .'
alias update='sudo apt-get update; sudo apt-get upgrade; sudo apt-get autoremove'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias g='googler --count 4'
alias off='sudo python3.5 ~/light_test.py 0 1'
alias on='sudo python3.5 ~/light_test.py 178 3'
